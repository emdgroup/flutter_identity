    "repository": {
        "type": "git",
        "url": "https://github.com/emdgroup/flutter_identity"
    },
    "author": "",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "standard-version": "^9.5.0",
        "yaml": "^2.2.2"
    }
}
